  pages = {1-7},
  file = {/media/jgschornak/Data/Zotero/storage/JK453M2B/Jonschkowski et al. - 2016 - Probabilistic multi-class segmentation for the Ama.pdf;/media/jgschornak/Data/Zotero/storage/EQM7Z8VR/7758087.html}
