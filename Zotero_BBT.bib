  number = {7},
  journal = {PLOS ONE},
  author = {Busse, Harald and Riedel, Tim and Garnov, Nikita and Th{\"o}rmer, Gregor and Kahn, Thomas and Moche, Michael},
  month = jul,
  year = {2015},
  keywords = {Magnetic resonance imaging,Biopsy,Navigation,Imaging techniques,Peas,Perturbation (geology),Radiologists,Screening guidelines},
  pages = {e0134370},
  file = {/media/jgschornak/Data/Zotero/storage/R6CLTWMJ/Busse et al. - 2015 - Targeting Accuracy, Procedure Times and User Exper.pdf;/media/jgschornak/Data/Zotero/storage/3WYVXQR7/article.html}
}

