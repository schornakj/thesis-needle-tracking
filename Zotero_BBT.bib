  pages = {819-824},
  file = {/media/jgschornak/Data/Zotero/storage/LDPQVRHN/Reed et al. - 2008 - Integrated planning and image-guided control for p.pdf;/media/jgschornak/Data/Zotero/storage/X3U2D95L/4762833.html}
