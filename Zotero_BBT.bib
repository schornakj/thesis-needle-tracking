  pages = {459-467},
  file = {/media/jgschornak/Data/Zotero/storage/S4M3T8GX/Glozman and Shoham - 2007 - Image-Guided Robotic Flexible Needle Steering.pdf;/media/jgschornak/Data/Zotero/storage/3V432LZ7/4252165.html}
