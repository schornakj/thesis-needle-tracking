  pages = {819-824},
  file = {/media/jgschornak/Data/Zotero/storage/PEBGZJ3L/Reed et al. - 2008 - Integrated planning and image-guided control for p.pdf;/media/jgschornak/Data/Zotero/storage/JM6MGBI9/4762833.html}
