  journal = {International Journal of Hyperthermia},
  author = {Negussie, Ayele H. and Partanen, Ari and Mikhail, Andrew S. and Xu, Sheng and Abi-Jaoudeh, Nadine and Maruvada, Subha and Wood, Bradford J.},
  month = apr,
  year = {2016},
  keywords = {thermochromic,HIFU,laser,microwave,RF,thermal ablation,thermal phantom,thermal therapy,tissue-mimicking},
  pages = {239-243},
  file = {/media/jgschornak/Data/Zotero/storage/DANCHWSE/02656736.2016.html},
  pmid = {27099078}
}

@article{zeng_robotic_2017,
