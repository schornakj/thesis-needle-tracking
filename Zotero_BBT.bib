  pages = {5896-5901},
  file = {/media/jgschornak/Data/Zotero/storage/8J43AJFS/Ayvali and Desai - 2014 - Accurate in-plane and out-of-plane ultrasound-base.pdf;/media/jgschornak/Data/Zotero/storage/42W2NTAJ/6907727.html}
