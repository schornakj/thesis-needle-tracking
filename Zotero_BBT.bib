  pages = {3476-3481},
  file = {/media/jgschornak/Data/Zotero/storage/DQY2G7DZ/Reed et al. - 2009 - Controlling a robotically steered needle in the pr.pdf;/media/jgschornak/Data/Zotero/storage/9EGABBDR/5152749.html}
