  pages = {4849-4854},
  file = {/media/jgschornak/Data/Zotero/storage/6HSDYJZX/Moreira et al. - 2017 - Towards MRI-guided flexible needle steering using .pdf;/media/jgschornak/Data/Zotero/storage/X72SHZYK/7989564.html}
