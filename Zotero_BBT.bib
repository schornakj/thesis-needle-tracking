  pages = {1083-1087},
  file = {/media/jgschornak/Data/Zotero/storage/Y67H2UM8/Kochavi et al. - 2004 - Method for rapid MRI needle tracking.pdf;/media/jgschornak/Data/Zotero/storage/73EATW4E/abstract.html}
}

