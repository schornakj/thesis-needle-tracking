  pages = {1398-1403},
  file = {/media/jgschornak/Data/Zotero/storage/4KU4QN8M/Ijspeert et al. - 2002 - Movement imitation with nonlinear dynamical system.pdf;/media/jgschornak/Data/Zotero/storage/2SAMDWPK/1014739.html}
