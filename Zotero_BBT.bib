  month = jan,
  year = {2017},
  keywords = {Prostate cancer,External beam radiotherapy,Fiducial markers,Multi-parametric MRI},
  pages = {14-20},
  file = {/media/jgschornak/Data/Zotero/storage/6TKAHU3Y/Dinis Fernandes et al. - 2017 - Prostate fiducial marker detection with the use of.pdf;/media/jgschornak/Data/Zotero/storage/QZVGCXPH/S2405631616300227.html}
