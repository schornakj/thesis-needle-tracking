  year = {2014},
  keywords = {needles,Needles,fibre optic sensors,Force,soft tissue,mechanics-based model,biomedical equipment,Bragg gratings,fiber Bragg grating sensors,Fiber gratings,Sensors,Shape,shape reconstruction,soft-tissue simulant,kinematics-based model,FBG sensors,strain measurement,Strain,3D needle shape,axial strain measurement,beam theory-based model,clinical imaging modalities,Fiber Bragg gratings (FBGs),flexible nitinol needle,in-plane deflection,kinematics-based models,mechanics-based models,needle curvature,needle deflection models,three-dimensional needle shape reconstruction},
  pages = {1115-1126},
  file = {/media/jgschornak/Data/Zotero/storage/3HK3H4J3/Roesthuis et al. - 2014 - Three-Dimensional Needle Shape Reconstruction Usin.pdf;/media/jgschornak/Data/Zotero/storage/WGCUI7UX/6558488.html}
